class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int XOR=0;
        for(auto it:nums){
            XOR=XOR^it;
        } 
        return XOR;     
    }
};//  tc=0(nums.size()) , sc=0(1)

// here another approach is by using map 
//we can also use 2 loops and check one by one
